{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "class-methods-use-this": "off",
    "linebreak-style": "off",
    "no-param-reassign": "off",
    "no-underscore-dangle": "off"
  }
}
